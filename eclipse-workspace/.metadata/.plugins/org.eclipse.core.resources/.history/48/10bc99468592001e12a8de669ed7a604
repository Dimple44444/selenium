package StringDemoAssignment6;

public class ChangeExistingValue {


	public static void main(String[] args) {
		
		
		String s1= "Welcome to the world of wonder";
		String s2= s1;
		
		System.out.println(s1);
		s1= s1.replace("to" , "a");
		System.out.println(s1);
		System.out.println(s2)
		;

	}

}
